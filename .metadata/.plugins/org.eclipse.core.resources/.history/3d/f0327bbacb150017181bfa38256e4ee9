//map方式一
<#list map?keys as k>
	${map[k].id}
	${map[k].name}
</#list>

//方式二
${map.m1.id}
${map.m1.name}
${map.m2.id}
${map.m2.name}


//List<Map>方式一
<#list lm as l >
	<#list l?keys as k>
		${l[k].id}
		${l[k].name}
	</#list>
</#list>

//方式二
<#list lm as l >
	${l.m1.id}
	${l.m1.name}
	${l.m2.id}
	${l.m2.name}
</#list>

